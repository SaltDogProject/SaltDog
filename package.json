        "husky": "^7.0.4",
        "lint-staged": "^11.2.3",
        "lodash": "^4.17.21",
        "pdfjs-dist": "^2.10.377",
        "puppeteer-core": "^10.4.0",
        "sbd": "^1.0.19",
        "stylus": "^0.55.0",
        "stylus-loader": "3.0.2",
        "translate-google": "^1.5.0",
        "vue": "^3.2.20",
        "vue-class-component": "^8.0.0-0",
        "vue-router": "^4.0.0-0",
        "vuex": "^4.0.0-0"
