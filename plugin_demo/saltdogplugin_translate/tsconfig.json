{
    "allowSyntheticDefaultImports": true,
    "compilerOptions": {
        "target": "es2020",
        "module": "esnext",
        "strict": true,
        "jsx": "preserve",
        "importHelpers": true,
        "moduleResolution": "node",
        "experimentalDecorators": true,
        "resolveJsonModule": true,
        "skipLibCheck": true,
